{
  "name": "@storystore/pwa-wknd",
  "version": "0.1.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "lint": "next lint",
    "dev": "NODE_ENV=development node server.js",
    "build": "next build",
    "xstart": "node server.js",
    "start": "electron .",
    "prepare": "echo ✔️",
    "cleanup": "echo ✔️",
    "prepkg": "rimraf .pkg && next build",
    "pkg": "nexe -t v12.9.1 -o .pkg/WKND.app -r \".next/**/*\" -r \"public/**/*\" -r \"../../node_modules/@storystore/{ui-kit,next-apollo,toolbox}/{*.js,package.json}\" -r \"../../node_modules/@storystore/{ui-kit,next-apollo,toolbox}/dist/**/*\" --verbose",
    "postpkg": "copyfiles -a .next/server/pages/*.html .env.local .pkg/ && chmod 774 .pkg/WKND.app"
  },
  "dependencies": {
    "@apollo/client": "^3.1.1",
    "@storystore/next-apollo": "^0.1.0",
    "@storystore/toolbox": "^0.1.0",
    "@storystore/ui-kit": "^0.1.0",
    "graphql": "^15.3.0",
    "http-proxy-middleware": "^2.0.1",
    "next": "^11.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^9.0.1",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "electron": "^13.2.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.0",
    "nexe": "^3.3.7",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.2.24",
    "next-transpile-modules": "^8.0.0",
    "rimraf": "^3.0.2",
    "webpack-open-browser": "^1.0.5"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
