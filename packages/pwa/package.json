{
  "name": "@storystore/pwa",
  "version": "1.0.4",
  "license": "MIT",
  "bin": "bin/server.js",
  "author": "Carlos A. Cabrera @fnhipster",
  "description": "An AEM PWA Experience",
  "scripts": {
    "lint": "next lint",
    "dev": "PORT=6007 next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "echo ✔️",
    "cleanup": "rimraf pkg .next",
    "prepkg": "rimraf pkg ; next build",
    "pkg": "pkg . --no-bytecode --public-packages \"*\" --public --compress GZip --output pkg/PWA"
  },
  "dependencies": {
    "@apollo/client": "^3.1.1",
    "@storystore/next-apollo": "^0.1.0",
    "@storystore/toolbox": "^0.1.0",
    "@storystore/ui-kit": "^1.0.2",
    "graphql": "^15.3.0",
    "http-proxy-middleware": "^2.0.1",
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-router-dom": "^5.3.0",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@types/xmldom": "^0.1.31",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.0",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.2.24",
    "pkg": "^5.3.1",
    "rimraf": "^3.0.2",
    "webpack-assets-manifest": "^4.0.6"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "pkg": {
    "assets": [
      ".env.local",
      ".next/**/*",
      "public/**/*",
      "../../node_modules/@surma/rollup-plugin-off-main-thread/loader.ejs",
      "../../node_modules/ts-invariant/process/*",
      "../../node_modules/remixicon-react/*",
      "../../node_modules/react-hook-form/**/*",
      "../../node_modules/@apollo/client/link/context/*",
      "../../node_modules/@adobe/**/*"
    ],
    "targets": [
      "node12-macos-x64"
    ]
  }
}
