{
  "name": "@storystore/next-apollo",
  "version": "0.1.0",
  "description": "An Apollo Client wrapper for Next.js",
  "author": "Carlos A. Cabrera @fnhipster",
  "license": "ISC",
  "main": "index.js",
  "files": [
    "README.md",
    "dist",
    "*.d.ts",
    "*.js"
  ],
  "scripts": {
    "build:babel": "cross-env BABEL_ENV=production babel --extensions \".js,.jsx,.ts,.tsx\" src -d dist --copy-files --no-copy-ignored",
    "build:types": "tsc  --emitDeclarationOnly",
    "dev": "concurrently \"yarn build:babel -- --watch\" \"yarn build:types -- --watch\"",
    "prepare": "yarn run build:babel && yarn run build:types",
    "cleanup": "rimraf dist"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.20",
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/runtime": "^7.14.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/lodash.isequal": "^4.5.5",
    "@types/react": "^17.0.11",
    "babel-preset-react-app": "^10.0.0",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "lodash.isequal": "^4.5.0",
    "graphql": "^15.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
