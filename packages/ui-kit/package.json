{
  "name": "@storystore/ui-kit",
  "version": "0.1.1",
  "description": "A UI Component Library",
  "author": "Carlos A. Cabrera @fnhipster",
  "license": "ISC",
  "main": "components",
  "files": [
    "README.md",
    "dist",
    "*.d.ts",
    "*.js"
  ],
  "scripts": {
    "build:babel": "cross-env BABEL_ENV=production babel --extensions \".js,.jsx,.ts,.tsx,.mdx\" src -d dist --copy-files --no-copy-ignored",
    "build:types": "tsc  --emitDeclarationOnly",
    "build:watch": "concurrently \"yarn build:babel -- --watch\" \"yarn build:types -- --watch\"",
    "prepare": "yarn run build:babel && yarn run build:types",
    "dev": "concurrently --names \"BUILD,STORYBOOK\" \"yarn build:watch\" \"start-storybook -p 6006 --no-manager-cache --quiet -s ./.storybook/assets\"",
    "build-storybook": "build-storybook",
    "cleanup": "rimraf dist"
  },
  "peerDependencies": {
    "@apollo/client": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.1.1",
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.14.6",
    "@react-aria/interactions": "^3.5.0",
    "@react-aria/tabs": "^3.0.0",
    "@react-hook/intersection-observer": "^3.1.1",
    "@react-stately/collections": "^3.3.2",
    "@react-stately/tabs": "^3.0.0",
    "@react-types/tabs": "^3.0.0",
    "@storybook/addon-actions": "^6.3.0",
    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/addon-links": "^6.3.0",
    "@storybook/react": "^6.3.0",
    "@storystore/storybook-variables": "0.1.0",
    "@storystore/toolbox": "^0.1.0",
    "@types/react-content-loader": "^4.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "http-proxy-middleware": "^2.0.1",
    "react": "^17.0.2",
    "react-content-loader": "^6.0.3",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.12.2",
    "react-toastify": "^8.0.0",
    "remixicon": "^2.5.0",
    "rimraf": "^3.0.2",
    "storybook-css-modules-preset": "^1.1.1",
    "typescript": "^4.3.4"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
