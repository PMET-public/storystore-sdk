{
  "compilerOptions": {
    "outDir": "dist",
    "target": "esnext",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "declaration": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true
  },
  "include": [
    "src",
    ".storybook",
    "typings"
  ],
  "exclude": [
    "node_module",
    "dist"
  ]
}