{
  "name": "@storystore/sdk",
  "version": "1.0.1",
  "private": true,
  "workspaces": [
    "packages/toolbox",
    "packages/storybook-variables",
    "packages/ui-kit",
    "packages/next-apollo",
    "packages/pwa"
  ],
  "devDependencies": {
    "concurrently": "^6.2.0",
    "prettier": "^2.3.1"
  },
  "scripts": {
    "cleanup": "yarn workspaces run cleanup",
    "prepare": "yarn workspaces run prepare",
    "sdk": "concurrently --names \"UIKit,PWA\" \"yarn workspace @storystore/ui-kit dev\" \"yarn workspace @storystore/pwa dev\"",
    "pkg": "yarn prepare && yarn workspace @storystore/pwa pkg"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
